    <!-- Regional Capital -->
    <Placemark>
      <name>Mekelle</name>
      <description><![CDATA[
        <h3>Mekelle ( መቐለ)</h3>
        <p><strong>Type:</strong> Regional Capital & Special Zone</p>
        <p><strong>Population:</strong> ~500,000 (2019)</p>
        <p><strong>Elevation:</strong> 2,254 meters</p>
        <p><strong>Significance:</strong> Capital and largest city of Tigray Region, major economic and educational center</p>
        <p><strong>Features:</strong> Mekelle University, Airport, Regional Government Headquarters</p>
      ]]></description>
      <styleUrl>#regionalCapital</styleUrl>
      <Point>
        <coordinates>39.4753,13.4967,2254</coordinates>
      </Point>
    </Placemark>

    <!-- Historic Cities -->
    <Placemark>
      <name>Axum</name>
      <description><![CDATA[
        <h3>Axum (አክሱም)</h3>
        <p><strong>Type:</strong> Historic City & UNESCO World Heritage Site</p>
        <p><strong>Population:</strong> ~60,000</p>
        <p><strong>Elevation:</strong> 2,131 meters</p>
        <p><strong>Significance:</strong> Ancient capital of Axumite Kingdom, religious center</p>
        <p><strong>Features:</strong> Stelae Field, Church of St. Mary of Zion, Archaeological sites</p>
        <p><strong>Zone:</strong> Central Tigray</p>
      ]]></description>
      <styleUrl>#historicCity</styleUrl>
      <Point>
        <coordinates>38.7236,14.1211,2131</coordinates>
      </Point>
    </Placemark>

    <Placemark>
      <name>Adwa</name>
      <description><![CDATA[
        <h3>Adwa (ዓድዋ)</h3>
        <p><strong>Type:</strong> Historic Town</p>
        <p><strong>Population:</strong> ~65,000</p>
        <p><strong>Elevation:</strong> 1,907 meters</p>
        <p><strong>Significance:</strong> Site of the famous Battle of Adwa (1896)</p>
        <p><strong>Features:</strong> Battle of Adwa Memorial, Historic churches</p>
        <p><strong>Zone:</strong> Central Tigray</p>
      ]]></description>
      <styleUrl>#historicCity</styleUrl>
      <Point>
        <coordinates>38.9000,14.1700,1907</coordinates>
      </Point>
    </Placemark>

