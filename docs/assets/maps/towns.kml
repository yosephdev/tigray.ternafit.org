    <!-- Regional Capital Style -->
    <Style id="regionalCapital">
      <IconStyle>
        <Icon>
          <href>http://maps.google.com/mapfiles/kml/pushpin/red-pushpin.png</href>
        </Icon>
        <scale>1.2</scale>
      </IconStyle>
      <LabelStyle>
        <scale>1.1</scale>
      </LabelStyle>
    </Style>
    
    <!-- Historic City Style -->
    <Style id="historicCity">
